{
    "name": "@elizaos/agent",
    "version": "0.1.9-alpha.1",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start": "tsx src/index.ts",
        "dev": "tsx src/index.ts",
        "check-types": "tsc --noEmit",
